<script type="text/javascript">


    $(document).ready(function () {
/*per img start*/
        var img = $('.myImg');
        var modal = $('#myModal');
        var modalImg =    $('#img01');
        var captionText =   $('#caption');
        for(var i = 0, len = img.length; i < len; i++) {
            img[i].onclick = function () {
                modal.css('display','block');
                modalImg.attr('src',this.src) ;
                captionText.html(this.alt);
            }
        }

        var span =  $('.close');
        span.click(function() {
            modal.css('display', "none");
        });
/*per img end*/

        let totalPrice = $('#totalPrice');
        let order_height = $('#order_height');
        let order_width = $('#order_width');
        let order_quantity = $('#order_quantity');
        let order_typee =  $('#order_typee');
        function calculPrice(){
            tp=(((parseInt(order_height.val())*parseInt(order_width.val()))/10000)
                *parseInt($('#typeID'+$('#order_typee option:selected').val()+'').html()))*parseInt(order_quantity.val());

            totalPrice.html(tp.toFixed(2));
            if(isNaN(totalPrice.html())) {
                totalPrice.html('Remplissez toutes les options');
            }
        };
        order_typee.change(function(){
            calculPrice();
        });
        order_height.change(function() {
            calculPrice();
        });
        order_width.change(function() {
            calculPrice();
        });
        order_quantity.change(function() {
            calculPrice();
        });






        let header = $('#header1');
        let count=0

        let imgUrl = '{{ asset('assets/uploads/images/background') }}'
        setInterval(function() {
            count++;
            if(count > 3){
                count=0;
            }
            header.attr('style','background-image: url(..'+imgUrl+count+'.jpg);');
        }, 15000);


        const menuDown = $('#menuDown');

        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
        });
    });

</script>
